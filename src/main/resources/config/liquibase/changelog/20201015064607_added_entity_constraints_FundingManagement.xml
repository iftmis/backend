<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity FundingManagement.
    -->
    <changeSet id="20201015064607-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_clusters_id"
                                 baseTableName="funding_management"
                                 constraintName="fk_funding_management_the_clusters_id"
                                 referencedColumnNames="id"
                                 referencedTableName="the_clusters"/>

        <addForeignKeyConstraint baseColumnNames="finding_sub_category_id"
                                 baseTableName="funding_management"
                                 constraintName="fk_funding_management_finding_sub_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="finding_sub_categories"/>

        <addForeignKeyConstraint baseColumnNames="financial_year_id"
                                 baseTableName="funding_management"
                                 constraintName="fk_funding_management_financial_year_id"
                                 referencedColumnNames="id"
                                 referencedTableName="financial_years"/>

        <addForeignKeyConstraint baseColumnNames="organisation_unit_level_id"
                                 baseTableName="funding_management"
                                 constraintName="fk_funding_management_organisation_unit_level_id"
                                 referencedColumnNames="id"
                                 referencedTableName="organisation_unit_levels"/>

    </changeSet>
</databaseChangeLog>
